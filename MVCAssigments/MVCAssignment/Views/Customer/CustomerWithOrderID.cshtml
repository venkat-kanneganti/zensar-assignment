@model IEnumerable<MVCAssignment.Models.Customer>
@{
    ViewBag.Title = "CustomerWithOrderID";
}

<h2>CustomerWithOrderID</h2>
@foreach (var item in Model)
{
    <li>@item.CustomerID</li>
    <li>@item.CompanyName</li>
    <li>@item.ContactName</li>
    <li>@item.ContactTitle</li>
    <li>@item.Address</li>
    <li>@item.City</li>
    <li>@item.PostalCode</li>
    <li>@item.Country</li>
    <li>@item.Phone</li>
    <li>@item.Fax</li>
}
